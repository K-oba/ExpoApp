<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_above="@+id/filterExpoFloatingButton"
    android:background="@color/caldroid_white"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.kaoba.expocr.activities.WelcomeActivity"
    tools:showIn="@layout/app_bar_welcome">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/calendarFloatingButton"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="224dp"
        android:clickable="true"
        app:fabSize="mini"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_menu_camera" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/filterExpoFloatingButton"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginLeft="161dp"
        android:layout_marginStart="161dp"
        android:layout_marginTop="224dp"
        android:clickable="true"
        app:backgroundTint="@color/Blueexpo"
        app:fabSize="mini"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_menu_camera" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/simpleListExpoButton"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginLeft="307dp"
        android:layout_marginStart="307dp"
        android:layout_marginTop="224dp"
        android:clickable="true"
        app:backgroundTint="@color/pinkexpo"
        app:fabSize="mini"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_menu_camera" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/QAFloatingButton"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginLeft="307dp"
        android:layout_marginStart="307dp"
        android:layout_marginTop="399dp"
        android:clickable="true"
        app:fabSize="mini"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_menu_camera" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/stadisticsButton"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="399dp"
        android:clickable="true"
        app:backgroundTint="@color/blueexpo"
        app:fabSize="mini"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_menu_camera" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/timelineButton"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_marginLeft="161dp"
        android:layout_marginStart="161dp"
        android:layout_marginTop="399dp"
        android:clickable="true"
        app:backgroundTint="@color/pinkexpo"
        app:fabSize="mini"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_menu_camera" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="34dp"
        android:layout_height="19dp"
        android:text="@string/live"
        android:textColor="@color/caldroid_black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        app:layout_constraintHorizontal_bias="0.726"
        app:layout_constraintVertical_bias="0.568" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="51dp"
        android:layout_height="19dp"
        android:text="@string/search"
        android:textColor="@color/caldroid_black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.568" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="34dp"
        android:layout_height="18dp"
        android:layout_marginLeft="344dp"
        android:layout_marginStart="337dp"
        android:text="@string/qa"
        android:textAlignment="center"
        android:textColor="@color/caldroid_black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.892" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="71dp"
        android:layout_height="17dp"
        android:text="@string/timeline"
        android:textAlignment="center"
        android:textColor="@color/caldroid_black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.892" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="71dp"
        android:layout_height="17dp"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="32dp"
        android:text="@string/statistics"
        android:textColor="@color/caldroid_black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.892" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="97dp"
        android:layout_height="18dp"
        android:text="@string/expo_calendar"
        android:textColor="@color/caldroid_black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.57"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.079" />

    <ImageView
        android:id="@+id/imageWelcomeHeader"
        android:layout_width="404dp"
        android:layout_height="143dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_menu_camera"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.428" />

</android.support.constraint.ConstraintLayout>
